<h1>Listing pessoajuridicas</h1>

<table>
  <tr>
    <th>Cnpj</th>
    <th>Observacoes</th>
    <th>Beneficiario</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pessoajuridicas.each do |pessoajuridica| %>
  <tr>
    <td><%= pessoajuridica.cnpj %></td>
    <td><%= pessoajuridica.observacoes %></td>
    <td><%= pessoajuridica.beneficiario %></td>
    <td><%= link_to 'Show', pessoajuridica %></td>
    <td><%= link_to 'Edit', edit_pessoajuridica_path(pessoajuridica) %></td>
    <td><%= link_to 'Excluir', pessoajuridica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>


<h1>Listing gastos</h1>

<table>
  <tr>
    <th>Data</th>
    <th>Valor</th>
    <th>Observacoes</th>
    <th>Rubrica</th>
    <th>Beneficiario</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @gastos.each do |gasto| %>
  <tr>
    <td><%= gasto.data %></td>
    <td><%= gasto.valor %></td>
    <td><%= gasto.observacoes %></td>
    <td><%= gasto.rubrica %></td>
    <td><%= gasto.beneficiario %></td>
    <td><%= link_to 'Show', gasto %></td>
    <td><%= link_to 'Edit', edit_gasto_path(gasto) %></td>
    <td><%= link_to 'Destroy', gasto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Gasto', new_gasto_path %>

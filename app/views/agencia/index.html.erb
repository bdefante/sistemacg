<h1>Listing agencia</h1>

<table>
  <tr>
    <th>Numero agencia</th>
    <th>Endereco</th>
    <th>Banco</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @agencia.each do |agencium| %>
  <tr>
    <td><%= agencium.numero_agencia %></td>
    <td><%= agencium.endereco %></td>
    <td><%= agencium.banco %></td>
    <td><%= link_to 'Show', agencium %></td>
    <td><%= link_to 'Edit', edit_agencium_path(agencium) %></td>
    <td><%= link_to 'Excluir', agencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />


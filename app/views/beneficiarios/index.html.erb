<h1>Listing beneficiarios</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Contum</th>
    <th>Endereco</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @beneficiarios.each do |beneficiario| %>
  <tr>
    <td><%= beneficiario.nome %></td>
    <td><%= beneficiario.contum %></td>
    <td><%= beneficiario.endereco %></td>
    <td><%= link_to 'Show', beneficiario %></td>
    <td><%= link_to 'Edit', edit_beneficiario_path(beneficiario) %></td>
    <td><%= link_to 'Excluir', beneficiario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Beneficiario', new_beneficiario_path %>
